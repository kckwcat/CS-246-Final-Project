Import Game;
bool isServer(Pos pos, int playerID){
	if(pos.r == playerID){
		if(pos.c == 3 || pos.c == 4){
			return true;
		}
	}
	return false;
}

void display(){
	for(int I=0; I<8; I++){
		for(int j=0; j<8; j++){
			cout << grid[i][j];
		}	
	}
	for(int I=0; I<8; j++){
		for(int j=0; j<8; j++){
			cout << firewalls[i][j];
		}
	}
}

import <string>;
import <vector>;
import <memory>;
import <algorithm>;
import Game;
public CommandParser{
unique_ptr<Game> game;
public:
void abilitySequence(){
	game->getCurrentPlayer().playAbility();
}
void run(){
	game->start();
}
void execute(string cmd){
	game->getCurrentPlayer();
	game->nextTurn();
}
void quit(){
	game->endGame();
}
}
